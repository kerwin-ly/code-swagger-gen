{
  "name": "ts-api-gen",
  "version": "0.0.1",
  "description": "A CLI tool to generate TypeScript Api code, based on swagger.json",
  "main": "index.js",
  "bin": {
    "ts-api-gen": "./bin/entry.js"
  },
  "scripts": {
    "dev": "rollup -w -c",
    "build": "rollup -c",
    "lint:ts": "tslint --force -p tsconfig.json -c tslint.json 'src/**/*.ts'",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kerwin-ly/ts-api-gen.git"
  },
  "keywords": [
    "typescript",
    "schema",
    "json",
    "swagger",
    "api"
  ],
  "author": "kerwin-ly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kerwin-ly/ts-api-gen/issues"
  },
  "homepage": "https://github.com/kerwin-ly/ts-api-gen#readme",
  "dependencies": {
    "commander": "^9.2.0",
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.32",
    "jest": "^28.1.0",
    "rollup": "^2.74.1",
    "rollup-plugin-delete": "^2.0.0",
    "ts-jest": "^28.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4"
  }
}
